<?xml version="1.0"?>
<launch>
<arg name="Abot_urdf_file" default="$(find xacro)/xacro.py '$(find dql_robot)/urdf/ABot.urdf.xacro'" />
<group ns="prey">
  <!-- send table urdf to param server -->
 <param name="robot_description" command="$(arg Abot_urdf_file)" />

  <!-- push table_description to factory and spawn robot in gazebo -->
  <node name="spawn_prey_model" pkg="gazebo_ros" type="spawn_model"
        args="$(optenv PREY_INITIAL_POSE) -urdf -param robot_description -model prey -y 2"/>
  
<node pkg="robot_state_publisher" name="publisher_state_pub" type="robot_state_publisher">
<param name="publish_frequency" type="double" value="30.0" />
</node>
</group>
</launch>
